{
  "id": "page",
  "label": "Page",
  "repeatable": true,
  "status": true,
  "format": "custom",
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID",
          "placeholder": ""
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "",
          "allowTargetBlank": true,
          "single": "heading1"
        }
      },
      "content": {
        "type": "StructuredText",
        "config": {
          "label": "Content",
          "placeholder": "Write your page content here...",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "main_image": {
        "type": "Image",
        "config": {
          "label": "Main Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "call_to_action": {
              "type": "SharedSlice"
            },
            "alternate_grid": {
              "type": "SharedSlice"
            },
            "customer_logos": {
              "type": "SharedSlice"
            },
            "hero": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO": {
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": "Title shown in search results"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "Description shown in search results"
        }
      },
      "meta_image": {
        "type": "Image",
        "config": {
          "label": "Meta Image",
          "constraint": {
            "width": 1200,
            "height": 630
          },
          "thumbnails": []
        }
      },
      "meta_keyword": {
        "type": "Text",
        "config": {
          "label": "meta keyword",
          "placeholder": "enter keywords seperated by commas"
        }
      },
      "slices1": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    }
  }
}
